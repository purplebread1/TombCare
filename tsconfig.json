{
	"extends": "expo/tsconfig.base",
	"compilerOptions": {
		"strict": true,
		"paths": {
			"@/*": ["./*"]
		}
	},
	"include": [
		"**/*.ts",
		"**/*.tsx",
		".expo/types/**/*.ts",
		"expo-env.d.ts",
		"app/(tabs)/home/index.js",
		"app/(tabs)/explore.js",
		"app/(tabs)/_layout.js",
		"app/+not-found.js",
		"app/_layout.js",
		"constants/Colors.js"
	]
}
